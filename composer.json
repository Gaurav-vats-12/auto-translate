{
    "name": "devaslanphp/auto-translate",
    "description": "Auto generate translation JSON files",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devaslanphp\\AutoTranslate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "DevaslanPHP"
        }
    ],
    "require": {
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "spatie/laravel-package-tools": "^1.14.0",
        "stichoza/google-translate-php": "^4.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devaslanphp\\AutoTranslate\\AutoTranslateProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}
